				function showCalc() {
					$('#popup-calc').show(500, null);
				}
				
				function hideCalc() {
					$('#popup-calc').hide(500, null);
				}		
				
				function calcWithCrowd(amount, crowd, percent, days) {
					var profit = 0;					
					for (i = 0; i < days; ++i) {
						var day_profit = (amount * percent / 100);
						profit = profit + day_profit;
						amount = amount + (day_profit * crowd / 100);
					}					
					return profit;					
				}

				function calculateProfit() {
					var amount = parseInt($('#popup-calc-amount').val());
					var plan = $('#popup-calc-plan').val();
					var crowdfunding = parseInt($('#popup-calc-crowdfunding').val());
					var profitElement = $('#popup-calc-profit');
					var profitPElement = $('#popup-calc-profit-p');  
					
					var profit = 0;					
					
					switch (plan) {
						case '1':
							profit = calcWithCrowd(amount, crowdfunding, 6, 18);
							break;
						case '2':
							profit = calcWithCrowd(amount, crowdfunding, 10, 12);
							break;
						case '3':
							profit = calcWithCrowd(amount, crowdfunding, 20, 6);
							break;
						case '4':
							profit = calcWithCrowd(amount, crowdfunding, 40, 3);
							break;
						case '5':
							profit = calcWithCrowd(amount, crowdfunding, 200, 1);
							break;
						case '6':
							profit = calcWithCrowd(amount, crowdfunding, 1000, 1);
							break;
						case '7':
							profit = amount + amount * 3;
							break;
					}
					
					var profitP = 0;
					profitP = profit * 100 / amount;
					
					profit = profit.toFixed(2);
					profitElement.val(profit);
					
					profitP = profitP.toFixed(2);
					profitPElement.val(profitP);
				}
				
				function hideCalcMessage() {
					elem = $('#popup-calc-message');
					elem.hide(250, null);
				}
				function showCalcMessage(message) {
					elem = $('#popup-calc-message');
					elem.text(message);
					elem.show(250, null);
					setTimeout(hideCalcMessage, 3000);
				}
				
			
		</script>

